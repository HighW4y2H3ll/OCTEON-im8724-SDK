function GetCookieUtil (name) {
var result = null;
var myCookie = " " + document.cookie + ";";
var searchName = " " + name + "=";
var startOfCookie = myCookie.indexOf(searchName);
var endOfCookie;
if (startOfCookie != -1) {
startOfCookie += searchName.length;
endOfCookie = myCookie.indexOf(";", startOfCookie);
result = unescape(myCookie.substring(startOfCookie, endOfCookie));
}
return result;
}

function SetCookieUtil (name,value,expires,path,domain,secure) {
document.cookie = name + "=" + escape (value) +
((expires) ? "; expires=" + expires.toGMTString() : "") +
((path) ? "; path=" + path : "; path=/") +
((domain) ? "; domain=" + domain : "; domain="+document.location.hostname) +
((secure) ? "; secure" : "");
}

var today = new Date();
var oneDay = new Date(today.getTime() + 24 * 60 *60 * 1000);
var flash_upgrade = GetCookieUtil("flashUpgrade");

if (flashVersion < 7 && flash_upgrade != "true") {
