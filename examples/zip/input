<script language="JavaScript" type="text/javascript">
<!--
	ERROR_MESSAGE_INVALID_ID_PASSWORD = "We are unable to verify the information that you have entered.  Please be certain that you have entered your Online ID and Passcode correctly.\n\nFor more information about IDs or Passcodes, please use the ID and Passcode help links on the Home Page.";
	// WR13837 - Increase from '4-7' to '4-12'
	ERROR_MESSAGE_INVALID_PASSWORD_MODEL = "The passcode must be 4-12 numbers and/or letters, case sensitive. It can not contain special characters (i.e. !@#$%^&*()-+,.??:;). Please re-enter the passcode. For more information on the passcode, click on the passcode help icon.";
	// END WR13837
	// WR13837 - Increase from '5-20' to '5-25'
	ERROR_MESSAGE_INVALID_ID_MODEL = "The ID must be 5-25 characters. Please re-enter your Online ID. For more information about your Online ID, click on the Online ID help icon.";
	// END WR13837
	// WR13837 - Increase from '4-8' to '4-12'
	ERROR_MESSAGE_INVALID_PASSWORD_CA = "The passcode must be 4-12 numbers and/or letters. It cannot contain special characters (i.e. !@#$%^&*()-+,.?/:;). Please re-enter the passcode.  For more information about your passcode, click the passcode help icon.";
	// END WR13837
	// WR13837 - Increase from '6-20' to '5-25'
	ERROR_MESSAGE_INVALID_ID_CA = "The ID must be 5-25 characters. Please re-enter your Online ID. For more information about your Online ID, click the ID help icon.";
	// END WR13837
	ERROR_MESSAGE_EMPTY_FIELDS = "Please enter both your Online ID and your Passcode and try again.";
//   Prevent multiple sign in button clicks
var signincnt = 0;
var isNav = (navigator.appName.indexOf("Netscape") != -1);
var isIE = (navigator.appName.indexOf("Microsoft") != -1);
// ****************************************
// Get Remember Me (Online ID) setting from the cookie and set global var
// Expected format for cookie is [encrypted_id]:[prefilled_with*'s]
var rememberme_prefill = "";
var cname = "olb_signin_prefill=";
// Get the cookie value
var dcookie = document.cookie;
// Test value
//var dcookie = "olb_signin_prefill=abcdef10101023:*****1122; ";
var clen = dcookie.length;
var cbegin = 0;
while (cbegin < clen) {
	var vbegin = cbegin + cname.length;
	if (dcookie.substring(cbegin, vbegin) == cname) {
		var vend = dcookie.indexOf (";", vbegin);
		if (vend == -1) vend = clen;
		rememberme_prefill = unescape(dcookie.substring(vbegin, vend));
	}
	cbegin = dcookie.indexOf(" ", cbegin) + 1;
	if (cbegin == 0) break;
}
// Get the prefill value from the cookie
if (rememberme_prefill != "") {
	vlen = rememberme_prefill.length;
	var vcolon = rememberme_prefill.indexOf(":", 0) + 1;
	if (vcolon == 0) {
		// cookie was present but did not contain expected colon
		rememberme_prefill = "";
	}
	else {
		rememberme_prefill = rememberme_prefill.substring(vcolon, vlen);
	}
}

// ****************************************
//   Event handler for "Enter" key...
function initHandlers () {
	addhandlers( window );
	addhandlers( document );
	addhandlers( document.frmSignIn );
	addhandlers( document.frmSignIn.pc );
	// Reset id/pc if filled with *'s
	if (document.frmSignIn.id.value == "*******") {document.frmSignIn.id.value = ""}
	if (document.frmSignIn.pc.value == "*******") {document.frmSignIn.pc.value = ""}
	// Don't set focus if user has already begun entering id or pc
	if (document.frmSignIn.id.value == "" && document.frmSignIn.pc.value == "") {
		// Check Remember Me setting
		if (rememberme_prefill != "") {
			// Set id to prefill, set focus to pc box, check remb me box
			document.frmSignIn.id.value = rememberme_prefill;
			document.frmSignIn.pc.focus();
			document.frmSignIn.rembme.checked = true;
		} else {
			// no prefill, set focus to id field
			document.frmSignIn.id.focus();
		}
	}
}
function mainHandler( e ) {
	var whichField;
	if ( isNav ) {
		if ( e.target.name )
			whichField = e.target.name;
		Key = e.which;
	}
	if ( isIE ) {
		e = window.event;
		if ( e.srcElement && e.srcElement.name )
			whichField = e.srcElement.name;
		Key = e.keyCode;
		e.cancelBubble = true;  // do not pass up the chain
	}
	if (( Key == 13 ) && ( whichField == "pc" )) {
		doSignIn();
	}
}
function addhandlers(o) {
	if ( isNav )
		window.captureEvents(Event.KEYPRESS);
	o.onkeypress = mainHandler;
}
// State cookie functions
function getState() {

	
	var state = document.frmSignIn.state.options[document.frmSignIn.state.selectedIndex].value + "";
	//
	if (state == "") {
		alert("Please enter the location where your account is held and try again.");
		return state;
	}
	var cookieDate = new Date();
	var cookieExpireDays = 1461; // 4 years
	cookieDate.setTime(cookieDate.getTime() + (cookieExpireDays * 24 * 3600 * 1000));
	//
	if (state == "CN") {
		document.cookie = "country=" + state + ";expires=" + cookieDate.toGMTString() + ";path=/";
		//State is CN, kill state cookie
		document.cookie = "state=;expires=Fri, 13-Apr-1999 00:00:00 GMT;path=/";

	} else {
		//Set state cookie
		document.cookie = "state=" + state + ";expires=" + cookieDate.toGMTString() + ";path=/";
		//Kill country cookie
		document.cookie = "country=;expires=Fri, 13-Apr-1999 00:00:00 GMT;path=/";
	}
	return state;

}
function setState() {

	
	var state = document.frmSignIn.state.options[document.frmSignIn.state.selectedIndex].value + "";
	if (state == "") {
		return;
	}
	var cookieDate = new Date();
	var cookieExpireDays = 1461; // 4 years
	cookieDate.setTime(cookieDate.getTime() + (cookieExpireDays * 24 * 3600 * 1000));
	//
	if (state == "CN") {
		document.cookie = "country=" + state + ";expires=" + cookieDate.toGMTString() + ";path=/";
	} else {
		document.cookie = "state=" + state + ";expires=" + cookieDate.toGMTString() + ";path=/";
	}

	return;
}
function doUserIDHelp() {
	var state = getState();
	if (state == "") {
		return;
	} else if (state == "CA") {
		helpURL = "https://onlineid.bankofamerica.com/cgi-bin/sso.login.controller?state=CA&reason=assistCA";
		location.href = helpURL;
	
	} else if (state == "CN") {
		helpURL = "https://onlineid.bankofamerica.com/cgi-bin/sso.login.controller?state=CN&reason=assistCA";
		location.href = helpURL;

	} else if ((state == "WA") || (state == "ID")) {
		doSignIn();
	} else {
		helpURL = "https://onlineid.bankofamerica.com/cgi-bin/sso.login.controller?reason=assistModel&state=" + state;
		location.href = helpURL;
	}
}
function doPasscodeHelp() {
	var state = getState();
	if (state == "") {
		return;

	
	} else if ((state == "CA") || (state == "CN")) {
		document.frmSignIn.Access_ID.value = document.frmSignIn.id.value;
		document.frmSignIn.reason.value = "create_passcode_ca";
		document.frmSignIn.action = "https://onlineid.bankofamerica.com/cgi-bin/sso.login.controller";
		document.frmSignIn.submit();
	} else if ((state == "WA") || (state == "ID")) {
		doSignIn();
	} else {
		document.frmSignIn.Access_ID.value = document.frmSignIn.id.value;
		document.frmSignIn.reason.value = "create_passcode_model";
		document.frmSignIn.action = "https://onlineid.bankofamerica.com/cgi-bin/sso.login.controller";
		document.frmSignIn.submit();
	}
}
function doCreatePasscode() {
	var state = getState();
	if (state == "") {
		return;
	
	} else if ((state == "CA") || (state == "CN")) {
		document.frmSignIn.Access_ID.value = document.frmSignIn.id.value;
		document.frmSignIn.reason.value = "create_passcode_ca";
		document.frmSignIn.action = "https://onlineid.bankofamerica.com/cgi-bin/sso.login.controller";
		document.frmSignIn.submit();
	
	} else if ((state == "WA") || (state == "ID")) {
		location.href = "/state.cgi?section=generic&destination=nba/onlinebanking/createpasscode/";
		return;
	} else {
		document.frmSignIn.Access_ID.value = document.frmSignIn.id.value;
		document.frmSignIn.reason.value = "create_passcode_model";
		document.frmSignIn.action = "https://onlineid.bankofamerica.com/cgi-bin/sso.login.controller";
		document.frmSignIn.submit();
	}
}
function doSignIn() {
	// user already clicked Sign In
	if (signincnt > 0) {return;}
	var state = getState();
	if (document.frmSignIn.id.value == "*******" || document.frmSignIn.pc.value == "*******") {
		alert("Safeguarding your information - For your protection, we have replaced the ID you entered earlier with asterisks. To sign in, please type in your ID over the asterisks, enter your passcode and click Sign In.");
		return;
	}
	// Check to see if a Remember Me user partially typed over the prefilled ID
	if (rememberme_prefill != "" && document.frmSignIn.id.value != rememberme_prefill && document.frmSignIn.id.value.indexOf("*", 0) >= 0) {
		alert("The Online ID you entered is not the same as your saved ID. Please re-enter your entire ID.");
		return;
	}
	if (state == "") {
		return;

	
	} else if ((state == "CA") || (state == "CN")) {
		if (!doEditChecksCA(document.frmSignIn.id.value, document.frmSignIn.pc.value)) {return}
		document.frmSignIn.Customer_Type.value = "CA";
		document.frmSignIn.Access_ID.value = document.frmSignIn.id.value;
		document.frmSignIn.Current_Passcode.value = document.frmSignIn.pc.value;
		document.frmSignIn.action = "https://onlineid.bankofamerica.com/cgi-bin/sso.login.controller";
	} else if ((state == "WA") || (state == "ID")) {
		// WA / ID customers get redirected to external sign in page for NW
		if (confirm('Please click OK to go to the Online Banking Log On page for Washington and Idaho.  You will need your account number, social security number and password to sign in.  Click Cancel to return to the Bank of America home page.')) {
			location.href = "/state.cgi?section=signin";
		}
		return;
	} else {
		if (!doEditChecksModel(document.frmSignIn.id.value, document.frmSignIn.pc.value)) {return}
		document.frmSignIn.Customer_Type.value = "MODEL";
		document.frmSignIn.Access_ID.value = document.frmSignIn.id.value;
		document.frmSignIn.Current_Passcode.value = document.frmSignIn.pc.value;
		document.frmSignIn.action = "https://onlineid.bankofamerica.com/cgi-bin/sso.login.controller";
	}
	document.frmSignIn.reason.value = "";
	imgSignIn = new Image();
	imgSignIn.src = "/images/tracking/homepage_signin.gif";
	
	document.frmSignIn.id.value = "*******";
	document.frmSignIn.pc.value = "*******";
	signincnt++;
	document.frmSignIn.submit();
}
function doEditChecksModel (id, passcode) {
	if (id.length == 0 && passcode.length == 0)	{
		alert(ERROR_MESSAGE_EMPTY_FIELDS);
		return false;
	}
	// Skip ID edit check if ID was prefilled and still matches
	if (!(rememberme_prefill != "" && document.frmSignIn.id.value == rememberme_prefill)) {
		if (!isValidAccessIDModel(id)) {
			alert(ERROR_MESSAGE_INVALID_ID_MODEL);
			return false;
		}
	}
	if (!isValidPasswordModel(passcode)) {
		alert(ERROR_MESSAGE_INVALID_PASSWORD_MODEL);
		return false;
	}
	return true;  // ID and Passcode okay
}
function isValidAccessIDModel(accessIDString) {
  var accessID = strip(accessIDString, " ");
  if (!isAlphanumericOrUnderscore(accessID)) {
    return false;
  }
  if ((accessID.length < 5) || (accessID.length > 25)) { //WR13837 - Change maxlength from 20 to 25
    return false;
  }
  return true;
}
function isValidPasswordModel(passwordString) {
  var password = strip(passwordString, " ");
  // password must be alphanumeric
  if (!isAlphanumeric(password)) {
    return false;
  }
  // password must be 4 to 12 characters in length
  if ((password.length < 4) || (password.length > 12)) { //WR13837 - Change maxlength from 7 to 12
    return false;
  }
  //
  if ( password == "000000" ) {
    return false;
  }

  return true;
}
function doEditChecksCA (id, passcode) {
	if (id.length == 0 && passcode.length == 0)	{
		alert(ERROR_MESSAGE_EMPTY_FIELDS);
		return false;
	}
	// Skip ID edit check if ID was prefilled and still matches
	if (!(rememberme_prefill != "" && document.frmSignIn.id.value == rememberme_prefill)) {
		if (!isValidAccessIDCA(id))	{
			alert(ERROR_MESSAGE_INVALID_ID_CA);
			return false;
		}
	}
	if (!isValidPasswordCA(passcode)) {
		alert(ERROR_MESSAGE_INVALID_PASSWORD_CA);
		return false;
	}
	return true;  // ID and Passcode okay
}
function isValidAccessIDCA(accessIDString) {
  var accessID = strip(accessIDString, " ");
  if (!isAlphanumericOrUnderscore(accessID))  {
    return false;
  }
  if ((accessID.length < 5) || (accessID.length > 25))  {  //WR13837 - Change maxlength from 6-20 to 5-25
    return false;
  }
  return true;
}
function isValidPasswordCA(passwordString) {
  var password = strip(passwordString, " ");
  // password must be alphanumeric
  if (!isAlphanumeric(password)) {
    return false;
  }
  // password must be 4 to 12 characters in length
  if ((password.length < 4) || (password.length > 12)) {  //WR13837 - Change maxlength from 8 to 12
    return false;
  }
  return true;
}
// -->
</script>

