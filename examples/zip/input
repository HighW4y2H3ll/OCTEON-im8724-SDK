	<script language="JavaScript" type="text/javascript">

		

	    function view_demo_window() {

			var state = getState();
			if (state == "") {
				return;
			} else if ( state == 'CA' ) {
					var demo_url = 'http://' + location.host + '/onlinebanking/demo2/detect-ca.cfm';
					var win = window.open(demo_url, 'preWin','width=700,height=530,scrollbars=yes,resizable=1')
			} else if ( state == 'CN' ) {
					var demo_url = 'http://' + location.host + '/creditcards/index.cfm?template=cc_onlinebanking_ca';
					location.href = demo_url;
			} else {
					var demo_url = 'http://' + location.host + '/onlinebanking/demo2/detect-model.cfm';
					var win = window.open(demo_url, 'preWin','width=700,height=530,scrollbars=yes,resizable=1')
			}

        }


		
		function view_learnmore() {

			var state = getState();
			if (state == "") {
				return;
			} else if ( state == 'CN' ) {
					var learnmore_url = 'http://' + location.host + '/creditcards/index.cfm?template=cc_onlinebanking_ca';
			} else {
			//WR13541 - change adtrack link for Learn more
					var learnmore_url = 'http://' + location.host + '/adtrack/index.cgi?adlink=0003020520000000a583';
			}
			location.href = learnmore_url;
		}

		

		function view_enroll() {

			var state = getState();
			if (state == "") {
				return;
			
			//} else if ( state == 'CN' ) {
				//var enroll_url = 'http://' + location.host + '/onlinebanking/newuser/?statecheck=CN';
			} else {
				var enroll_url = 'http://' + location.host + '/adtrack/index.cgi?adlink=00030213700000006513';
			}
			location.href = enroll_url;
		}

	//-->
	</script>


	

	

	

