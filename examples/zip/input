<!--
function setMarketChartVariables() {
  if (document.images) { 
	globalCurrChart = 1;
	globalCurrTab = "";
	globalCurrSid = ""; 
	globalCurrSymb = "";
	globalCurrTime = "";
	globalDummyVariable = 0;
  }
}

function changeGraph(whichmap,whichchart){
	globalCurrChart = whichchart;
	updateGraph(whichmap);
}

function changeTab(which,whichmap){
	globalCurrChart = "";
	globalCurrTab = which;
	globalCurrSid = "";
	globalCurrSymb = "";
	updateGraph(whichmap);
}
			
function updateGraph(whichmap){
	if (document.images) {
	  document.images[whichmap].src = imagesource();
	}
}
				
function imagesource(){
	var temp = "http://chart.bigcharts.com/custom/nyt-com/global-market-chart.img?time=" + globalCurrTime + "&sid=";
	temp += globalCurrSid + "&symb=" + globalCurrSymb + "&index=" + globalCurrChart + "&which=" + globalCurrTab + "&refreshcount=" + globalDummyVariable;
	globalDummyVariable++;
	return temp;
}

function changeTime (whichmap,newTime) {
	globalCurrTime = newTime;
	updateGraph(whichmap);
}

setMarketChartVariables();
//-->
</script>
