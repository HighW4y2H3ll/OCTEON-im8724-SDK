// Tiny values.  If enabled, display values in a tiny font.
// This could be combined with a pattern like `*\[*\]' to make
// all array members tiny.

#pragma replace value_rm
#pragma replace value_bf
#pragma replace value_it
#pragma replace value_bi

value_rm(box) = tiny_rm(box);
value_bf(box) = tiny_bf(box);
value_it(box) = tiny_it(box);
value_bi(box) = tiny_bi(box);
